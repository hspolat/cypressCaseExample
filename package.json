{
  "name": "cypresscaseexample",
  "version": "1.0.0",
  "description": "simple automatized cases with cypress",
  "main": "index.js",
  "scripts": {
    "test": "cypress run --spec \"cypress/e2e/**/*\"",
    "test:chrome": "cypress run --spec \"cypress/e2e/**/*\" --browser chrome",
    "test:firefox": "cypress run --spec \"cypress/e2e/**/*\" --browser firefox",
    "test:all": "npm run test:chrome && npm run test:firefox"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hspolat/cypressCaseExample.git"
  },
  "keywords": [
    "cypress",
    "test",
    "automation",
    "web",
    "test",
    "UI",
    "test"
  ],
  "author": "huseyin polat",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/hspolat/cypressCaseExample/issues"
  },
  "homepage": "https://github.com/hspolat/cypressCaseExample#readme",
  "devDependencies": {
    "cypress": "^13.15.0",
    "cypress-mochawesome-reporter": "^3.8.2"
  }
}